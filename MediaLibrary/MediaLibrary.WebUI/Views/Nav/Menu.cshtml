@model IEnumerable<string>

@Html.ActionLink("Home", "Index", "Home")

@foreach (var link in Model)
{
    @Html.RouteLink(link, new {
        controller = "Song",
        action = "List",
        artist = link,
        page = 1    
    },
    new {
        @class = link == ViewBag.SelectedArtist ? "selected" : null
    })
}
